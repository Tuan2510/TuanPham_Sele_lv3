<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="VJ Test Suite" parallel="tests" thread-count="2">
    <listeners>
        <listener class-name="utils.TestListener"/>
        <listener class-name="io.qameta.allure.testng.AllureTestNg"/>
    </listeners>
    <groups>
        <run>
            <include name="VJRegression"/>
        </run>
    </groups>
    <test name="VietJet Test Suite EN">
        <parameter name="browser" value="chrome"/>
        <parameter name="language" value="en-us"/>
        <parameter name="env" value="stg"/>
        <classes>
            <class name="testcases.VJTest.VJTestSel3">
                <methods>
                    <include name="TC01"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>